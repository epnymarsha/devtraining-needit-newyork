        <name>what_needed_onLoad</name>
        <order/>
        <script><![CDATA[function onLoad() {
   //Type appropriate comment here, and begin script below
	
	var whatneeded = g_form.getValue('u_what_needed');
	var department = g_form.getValue('u_department');
	
   if(department != '')
   {
	if(whatneeded == 'HR'){
		g_form.addOption('u_what_needed', 'hr1','Humar Resource 1');
		g_form.addOption('u_what_needed', 'hr2','Humar Resource 2');
	}
	if(whatneeded == 'Facilities'){
		g_form.addOption('u_what_needed', 'f1','Facilities 1');
		g_form.addOption('u_what_needed', 'f2','Facilities 2');	
		
	}
	if(whatneeded == 'Legal'){
		g_form.addOption('u_what_needed', 'l1','Legal 1');
		g_form.addOption('u_what_needed', 'l2','Legal 2');		
		
	}
	}
}]]></script>
